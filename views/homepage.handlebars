{{!-- {{ #each postData as |post|}} --}}
<div class="card">
  <div class="card-header">
    {{ post.user_id }}
  </div>
  <div class="card-body">
    <h5 class="card-title"> {{ post.title }}</h5>
    <p class="card-text"> {{ post.content }}</p>
    <a href="#" class="btn btn-primary"> Make a Comment </a>
  </div>
</div>
{{!-- {{/each}} --}}